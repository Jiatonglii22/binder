<% if can?(:update, @organization_status) %>
  <%- model_class = OrganizationStatus -%>
  <div class="page-header">
    <h1>Edit Status Entry for <%= @organization.name %></h1>
  </div>
    
  <%= render 'form' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                [@organization, :organization_statuses], :class => 'btn btn-default' %>
  </div>
  
<% else %>
  <h3>Not Authorized!</h3>
<% end %>

